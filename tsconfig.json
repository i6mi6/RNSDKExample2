{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "noEmit": true,
    "strict": false,
    "target": "ESNext",
    "baseUrl": ".",
    "suppressExcessPropertyErrors": true,
    "ignoreDeprecations": "5.0",
  },
  "include": [
    "*",
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
